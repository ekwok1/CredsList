<div class="container-fluid">
  <h2 id="listings">All Listings</h2>
  <% @items.each do |item| %>
    <div><%= link_to item.name, item_path(item) %></div>
    <div><%= item.description %></div>
    <div><%= item.price %></div>
    <img src="<%= item.pic_url %>" alt="Profile Picture">
  <% end %>
</div>
